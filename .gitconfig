[user]
	name = <name>
	email = <email>
[color]
    ui = auto
[alias]
    aa = add -A
    co = checkout
    cm = commit -m
    st = status
    cp = cherry-pick
    br = branch
    ge = grep -e
    # log tree
    log-graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
    log-all = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'

[push]
    default = current
[pull]
    default = current
[core]
    whitespace = cr-at-eol
    editor = vim -c \"set fenc=utf-8\"
